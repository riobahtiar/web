{
  "name": "@riobahtiar/web",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "deno-deploy": "pnpm run build && cd apps/astro-blog/ && DENO_DEPLOY_TOKEN=<DENO_DEPLOY_TOKEN> deployctl deploy --project=<DENO_DEPLOY_PROJECT> --no-static --include=./dist ./dist/server/entry.mjs"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.9.5"
}